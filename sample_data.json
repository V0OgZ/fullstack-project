  "apiExamples": {
    "createGame": {
      "method": "POST",
      "url": "/api/game/create",
      "body": {
        "name": "My 5D Game",
        "maxPlayers": 4,
        "settings": {
          "timePerTurn": 180,
          "maxTimelines": 3,
          "maxPsiStatesPerPlayer": 8,
          "enableTemporalArtifacts": true,
          "enableMultipleTimelines": true,
          "enablePhantomBattles": true,
          "mapSize": "large",
          "difficulty": "hard"
        }
      }
    },
    "joinGame": {
      "method": "POST",
      "url": "/api/game/{gameId}/join",
      "body": {
        "playerId": "player123",
        "playerName": "Temporal Master",
        "color": "purple"
      }
    },
    "moveHero": {
      "method": "POST",
      "url": "/api/game/{gameId}/heroes/{heroId}/move",
      "body": {
        "x": 12,
        "y": 15,
        "z": 0,
        "timeline": "ℬ1",
        "temporalLayer": 0
      }
    },
    "useArtifact": {
      "method": "POST",
      "url": "/api/game/{gameId}/artifacts/{artifactId}/use",
      "body": {
        "heroId": "hero_arthur",
        "targetX": 126,
        "targetY": 65,
        "targetTimeline": "ℬ1",
        "targetTemporalLayer": 0
      }
    },
    "executeScript": {
      "method": "POST",
      "url": "/api/game/{gameId}/script/execute",
      "body": {
        "script": "ψ001: ⊙(Δt+2 @126,65 ⟶ CREATE(CREATURE, Dragon))\nUSE(ITEM, AvantWorldBlade, HERO:Arthur)",
        "playerId": "player1"
      }
    }
  },
  
